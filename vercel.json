{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "excludeFiles": "**/pydeck/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "app.py": {
            "excludeFiles": "**/pydeck/**"
        }
    }
}